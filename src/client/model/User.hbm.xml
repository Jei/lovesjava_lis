<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="client.model">
	<class name="User" table="User">
		<id name="id" column="USER_ID">
			<generator class="native"/>
		</id>
		<property name="name" column="USER_NAME" not-null="true"/>
		<property name="sname" column="USER_SNAME" not-null="true"/>
		<property name="email" column="USER_EMAIL" not-null="true"/>
		<property name="pass" column="USER_PASS" not-null="true"/>
		<property name="adm" column="USER_ADM" not-null="true"/>
		<property name="gender" column="USER_GENDER" not-null="false"/>
		<property name="birth" column="USER_BIRTH" not-null="false"/>
		<property name="cf" column="USER_CF" not-null="false"/>
		<property name="blocked" column="USER_BLOCKED" not-null="true"/>
	</class>
</hibernate-mapping> 